{
    "name": "webba/booking",
    "description": "Webba Booking Plugin",
    "type": "wordpress-plugin",
    "require": {
        "stripe/stripe-php": "^8.11",
        "paypal/rest-api-sdk-php": "*",
        "johngrogg/ics-parser": "^3",
        "twilio/sdk": "^6.39",
        "voku/anti-xss": "^4.1",
        "eluceo/ical": "^2.13",
        "mixpanel/mixpanel-php": "2.*",
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "yoast/phpunit-polyfills": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Webba\\Booking\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Webba\\Booking\\Tests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html coverage"
    }
}
